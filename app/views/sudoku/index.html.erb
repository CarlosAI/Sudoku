<script type="text/javascript">

	var escondidos = <%= @bloqueados %>;
	var los_chidos = <%= @los_chidos %>;
	console.log(escondidos);
	console.log("los chidos son");
	console.log(los_chidos);

	var contador = 0;

	$( document ).ready(function() {
		var numeros = <%= @los_numeros_finales %>;
	    //console.log(numeros);
	    //alert(numeros[0])
	    for (i = 0; i <= numeros.length; i++) {
	    	if(numeros[i]==0){
	    		$("#"+(i+1)).val("");
	    	}else{
	    		$("#"+(i+1)).val(numeros[i]);
	    	}
		  //console.log(numeros[i]);
		}

	});

	$('#prueba input').each(function() { 
	   $(this).on('change', function () {
	        //console.log($(this).attr('id'));
	        var id = $(this).attr('id');
	        var td = "td-"+id
	        //console.log(id-1);
	        var ids = id-1
	        console.log(ids);
	        console.log($(this).val());
	        console.log(los_chidos[ids]);
	        if($(this).val()==los_chidos[ids]){
	        	console.log("A webo");
	        	$("#"+td).addClass('succes');
	        	contador = contador + 1;
	        }else{
	        	console.log("nel");
	        	$("#"+td).addClass('error');
	        }
	        console.log(contador);
	        if(contador==escondidos.length){
	        	alert("FELCIDIDADES :V")
				$('#again').removeAttr('hidden');
			}
	    });
	});
</script>


